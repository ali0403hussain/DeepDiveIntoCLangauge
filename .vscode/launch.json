            "name": "Debug 01.HelloWorld",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/01.helloWorld/src/app/helloWorld.exe", 
            "cwd": "${workspaceFolder}/01.helloWorld", 
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "C:/msys64/mingw32/bin/gdb.exe",  
            "stopAtEntry": false,
            "externalConsole": false
